set(openai-c_VERSION 0.0.1)

if(NOT openai-c_FIND_VERSION_EXACT)
    if(openai-c_FIND_VERSION_LESS_EQUAL "${openai-c_VERSION}")
        set(PACKAGE_VERSION_COMPATIBLE TRUE)
        if(openai-c_FIND_VERSION VERSION_EQUAL "${openai-c_VERSION}")
            set(PACKAGE_VERSION_EXACT TRUE)
        endif()
    else()
        set(PACKAGE_VERSION_UNSUITABLE TRUE)
    endif()
else()
    if(openai-c_FIND_VERSION VERSION_EQUAL "${openai-c_VERSION}")
        set(PACKAGE_VERSION_EXACT TRUE)
        set(PACKAGE_VERSION_COMPATIBLE TRUE)
    else()
        set(PACKAGE_VERSION_UNSUITABLE TRUE)
    endif()
endif()